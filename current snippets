  <h2 class="sidebar-title">{{ 'collections.listing.current' | t }}</h2>
  <ul class="tags">
    {% for tag in collection.all_tags %}
      {% if current_tags contains tag %}
        <li class="tag show current">
          {{ tag | append: '<span class="remove">x</span>' | link_to: collection.url }}
        </li>
      {% endif %}
    {% endfor %}
  </ul>
{% endif %}


