<ul>
  {%
    render 'product-item',
    product: product,
    countdown_timer: true,
  %}
</ul>

{% comment %}
  @param countdown_year {String}
    The countdown timer year

  @param countdown_month {String}
    The countdown timer month

  @param countdown_day {String}
    The countdown timer day

  @param countdown_hour {String}
    The countdown timer hour

  @param countdown_minute {String}
    The countdown timer minute

  @param countdown_timer_background {Boolean}
    If true, show the countdown timer background

  @param countdown_timer_complete {Boolean}
    If true, hide the countdown timer when the timer expires

  @param countdown_timer_message {String}
    The message shown when the countdown timer has expired
{% endcomment %}

{%- capture datetime -%}
  {{- countdown_year | append: '-' -}}
  {{- countdown_month | append: '-' -}}
  {{- countdown_day | append: 'T' -}}
  {{- countdown_hour | append: ':' -}}
  {{- countdown_minute | append: ':00.000' -}}
{%- endcapture -%}

{% liquid
  assign timezone = datetime | date: "%z"
  assign datetime = datetime | append: timezone
%}

<div
  class="
    countdown
    {% if countdown_timer_background %}
      countdown--has-background
    {% endif %}
    {% if countdown_timer_complete %}
      countdown--hide-items
    {% endif %}
  "
  data-countdown
>
  <div class="countdown__items">
    <time
      class="
        countdown__item
        countdown__item--datetime
      "
      datetime="{{ datetime }}"
      data-time
    >
    </time>

    <div
      class="
        countdown__item
        countdown__item--days
      "
    >
      <h2 class="countdown__item-heading" data-days></h2>
      <span class="countdown__item-text">{{ 'sections.countdown-timer.days' | t }}</span>
    </div>

    <div
      class="
        countdown__item
        countdown__item--hours
      "
    >
      <h2 class="countdown__item-heading" data-hours></h2>
      <span class="countdown__item-text">{{ 'sections.countdown-timer.hours' | t }}</span>
    </div>

    <div
      class="
        countdown__item
        countdown__item--minutes
      "
    >
      <h2 class="countdown__item-heading" data-minutes></h2>
      <span class="countdown__item-text">{{ 'sections.countdown-timer.minutes' | t }}</span>
    </div>

    <div
      class="
        countdown__item
        countdown__item--seconds
      "
    >
      <h2 class="countdown__item-heading" data-seconds></h2>
      <span class="countdown__item-text">{{ 'sections.countdown-timer.seconds' | t }}</span>
    </div>
  </div>

  {% if countdown_timer_message != blank %}
    <div class="countdown__message">
      {{ countdown_timer_message }}
    </div>
  {% endif %}
</div>

<div id="coin-container"></div>

{% if settings.show-currency-switcher %}
  <div class="currency-switcher">
    <div class="selected-currency"></div>

    {% include 'icon' with 'down-arrow' %}

    <select id="currencies" name="currencies">
      {% capture codes %},USD,EUR,GBP,CAD,ARS,AUD,BBD,BDT,BSD,BHD,BRL,BOB,BND,BGN,MMK,KYD,CLP,CNY,COP,CRC,HRK,CZK,DKK,DOP,XCD,EGP,XPF,FJD,GHS,GTQ,GYD,GEL,HKD,HUF,ISK,INR,IDR,NIS,JMD,JPY,JOD,KZT,KES,KWD,LVL,LTL,MXN,MYR,MUR,MDL,MAD,MNT,MZN,ANG,NZD,NGN,NOK,OMR,PKR,PYG,PEN,PHP,PLN,QAR,RON,RUB,SAR,RSD,SCR,SGD,SYP,ZAR,KRW,LKR,SEK,CHF,TWD,THB,TZS,TTD,TRY,UAH,AED,UYU,VEB,VND,ZMK,{% endcapture %}
      {% assign supported_codes = settings.currency-switcher-supported-currencies | escape | split: ' ' %}

      <option value="{{ shop.currency }}" selected="selected">{{ shop.currency }}</option>
      {% for code in supported_codes %}
        {% if code != shop.currency and codes contains code %}
        <option value="{{ code }}">{{ code }}</option>
        {% endif %}
      {% endfor %}
    </select>
  </div>
{% endif %}

{% comment %}
  @param filter
  @param class_prefix
  @param forloop
{% endcomment %}

<div class="{{ class_prefix }}-filters__details-wrapper">
<details
  class="
    {{ class_prefix }}-filters__filter-group
  "
  aria-label="{{ filter.label }}"
  data-filter-group
>
  <summary
    class="{{ class_prefix }}-filters__filter-group-title"
    data-filter-group-summary
    data-filter-group-index="{{ forloop.index0 }}"
  >
    <span class="{{ class_prefix }}-filters__filter-title">
      {{ filter.label }}
      {%- if filter.type == 'list' and filter.active_values.size > 0 -%}
        ({{ filter.active_values.size | strip }})
      {% elsif filter.type == 'price_range' %}
        {%- if filter.max_value.value or filter.min_value.value -%}
          {% comment %}
            If there are any price filters applied, just indicate that there is a single
            active filter in this dropdown
          {% endcomment %}
          (1)
        {%- endif -%}
      {%- endif -%}
    </span>
    <span class="{{ class_prefix }}-filters__filter-chevron">
      {% render 'icon' with 'arrow-thin-right' %}
    </span>
  </summary>

  {% case filter.type %}
    {% when 'boolean' or 'list' %}
      <ul
        class="
          {{ class_prefix }}-filters__filter
          {{ class_prefix }}-filters__filter-list
        "
        data-filter-group-list
        data-filter-group-content
        aria-expanded="false"
        data-accordion-content
      >
        {%- for value in filter.values -%}
          <li
            class="
              {{ class_prefix }}-filters__filter-list-item
              {% if value.active %}
                {{ class_prefix }}-filters__filter-list-item--active
              {% endif %}
            "
          >
            <a
              class="{{ class_prefix }}-filters__filter-link"
              data-name="{{ value.param_name }}"
              data-value="{{ value.value }}"
              data-handle="filter-{{ filter.label | escape | replace: ' ', '-'}}-{{ forloop.index0 }}"

              {% if value.active -%}
                {% assign animation_state = 'checked' %}
                data-filter-active
                data-url="{{ value.url_to_remove | replace: '%2C', ',' }}"
                href="{{ value.url_to_remove | replace: '%2C', ',' }}"
              {% else %}
                  {% assign animation_state = 'unchecked' %}
                data-url="{{ value.url_to_add | replace: '%2C', ',' }}"
                href="{{ value.url_to_add | replace: '%2C', ',' }}"
              {%- endif %}
              {% if value.count == 0 and value.active == false -%}
                data-disabled
                tabindex="-1"
              {%- endif %}
              data-filter-input
            >
              {%- case filter.presentation -%}
                {%- when 'swatch' -%}
                  {%-
                    render 'faceted-filter-swatch',
                    type: value.display.type,
                    value: value.display.value,
                  -%}
                {%- when'image' -%}
                  <span class="faceted-filter-image">
                    {%- if value.image -%}
                      {{- value.image | image_url: width: 100 | image_tag: class: 'faceted-filter-image__image', alt: value.image.alt, loading: 'lazy' -}}
                    {%- endif -%}
                  </span>
                {%- else -%}
                  <span
                    class="{{ class_prefix }}-filters__filter-checkbox"
                    {% if value.active %}data-checked{% endif %}
                    data-filter-checkbox
                    data-handle="filter-{{ filter.label | escape | replace: ' ', '-'}}-{{ forloop.index0 }}"
                  >
                    {% render 'icon' with 'checkbox' %}
                  </span>
              {%- endcase -%}

              <span class="{{ class_prefix }}-filters__filter-list-item-text">
                {{ value.label | escape }}
                ({{ value.count }})
              </span>
            </a>
          </li>
        {%- endfor -%}
      </ul>
    {% when 'price_range' %}
      <div
        class="{{ class_prefix }}-filters__filter"
        data-filter-group-content
      >
        <form
          class="{{ class_prefix }}-filters__price-range-from">
          <div
            class="
              {{ class_prefix }}-filters__filter-range
            "
            data-filter-group-range
            aria-expanded="false"
          >
            <div class="{{ class_prefix }}-filters__filter-range--from">
              <span class="{{ class_prefix }}-filters__filter-range-currency">{{ cart.currency.symbol }}</span>
              <input
                class="
                  field
                  {{ class_prefix }}-filters__filter-range-input
                "
                name="{{ filter.min_value.param_name }}"
                id="filter-{{ filter.label }}-{{ forloop.index0 }}-from"
                {% if filter.min_value.value -%}
                  value="{{ filter.min_value.value | money_without_currency | replace: ',', '' }}"
                {%- endif %}
                type="number"
                inputmode="decimal"
                placeholder="{{ 'collections.listing.range_from' | t }}"
                aria-label="{{ 'collections.listing.range_from' | t }}"
                min="0"
                max="{{ filter.range_max | money_without_currency | replace: ',', '' }}"
                data-filter-range
              >
            </div>
            <div class="{{ class_prefix }}-filters__filter-range--to">
              <span class="{{ class_prefix }}-filters__filter-range-currency">{{ cart.currency.symbol }}</span>
              <input
                class="
                  field
                  {{ class_prefix }}-filters__filter-range-input
                "
                name="{{ filter.max_value.param_name }}"
                id="filter-{{ filter.label }}-{{ forloop.index0 }}-to"
                {% if filter.max_value.value -%}
                  value="{{ filter.max_value.value | money_without_currency | replace: ',', '' }}"
                {%- endif %}
                type="number"
                inputmode="decimal"
                placeholder="{{ 'collections.listing.range_to' | t }}"
                aria-label="{{ 'collections.listing.range_to' | t }}"
                min="0"
                max="{{ filter.range_max | money_without_currency | replace: ',', '' }}"
                data-filter-range
              >
            </div>
          </div>
        </form>
      </div>
  {% endcase %}
</details>
</div>

{% comment %}
  @param type {String}
    The type of visual representation, either "colors" or "image".

  @param value {Object}
    The visual representation, either a list of colors, or an image.
{% endcomment %}

<div class="faceted-filter-swatch">
  {%- case type -%}
    {%- when 'colors' -%}
      <div class="faceted-filter-swatch__color" style="--faceted-filter-swatch-color: {{ value }};"></div>

    {%- when 'image' -%}
      {{- value | image_url: width: 100 | image_tag: class: 'faceted-filter-swatch__image', alt: value.alt, loading: 'lazy' -}}
  {%- endcase -%}
</div>

{% comment %}
  @param filters {Iterable}
    Filters object - collection.filters or search.filters
  @param class_prefix {String}
    Classname prefix
  @param clear_url {String}
    URL for the 'clear filters' link
{% endcomment %}

{% assign filter_count = 0 %}

{%- capture tag_icon -%}
  <span class="filter-icon--remove">
    {% render 'icon' with 'close' %}
    <span class="visually-hidden">
      {{ 'collections.listing.remove' | t }}
    </span>
  </span>
{%- endcapture -%}

<ul class="
    active-filter-group
  "
>
  {%- for filter in filters -%}
    {% if filter.type == 'price_range' and filter.min_value.value or filter.max_value.value %}
      {% assign filter_count = filter_count | plus: 1 %}
      {% assign range_min = filter.min_value.value | default: 0 %}
      {% assign range_max = filter.max_value.value | default: filter.range_max %}

      {%- capture tag_text -%}
        <span class="active-filter-text">
          {{ range_min | money }} - {{ range_max | money }}
        </span>

        {{- tag_icon -}}
      {%- endcapture -%}

        <li class="active-filter-group__item-wrapper">
          <a
            class="active-filter-group__item"
            data-url="{{ filter.url_to_remove }}"
            href="{{ filter.url_to_remove | replace: '%2C', ',' }}"
          >
            {{ tag_text }}
          </a>
        </li>

    {% else %}
      {% if filter.active_values.size > 0 %}
          {%- for value in filter.active_values -%}
            {% assign filter_count = filter_count | plus: 1 %}
            <li class="active-filter-group__item-wrapper">
              {%- capture tag_text -%}
                <span class="active-filter-text">
                  {% if filter.type == 'boolean' %}
                    {{ filter.label | escape }}:
                  {% endif %}
                  {{ value.label | escape }}
                </span>

                {{- tag_icon -}}
              {%- endcapture -%}

              <a
                class="active-filter-group__item"
                data-url="{{ value.url_to_remove }}"
                href="{{ value.url_to_remove | replace: '%2C', ',' }}"
              >
                {{ tag_text }}
              </a>
            </li>
          {% endfor %}
      {% endif %}
    {% endif %}
  {% endfor %}
  {% if filter_count > 0 %}
      <li class="active-filter-group__item-wrapper">
        <a
          class="active-filter-group__item active-filter-group__item--clear"
          href="{{ clear_url }}"
        >
          <span class="active-filter-text sidebar-title">
            {{ 'collections.listing.clear_all' | t }}
          </span>
        </a>
      </li>
  {% endif %}
</ul>

{% comment %}
  @param filters {Iterable}
    Filters object - collection.filters or search.filters
  @param class_prefix {String}
    Classname prefix
{% endcomment %}

<div class="{{ class_prefix }}__filters-wrapper" data-faceted-filter>
  <label class="{{ class_prefix }}__filters-title sidebar-title">
    {{ 'collections.listing.filter-by' | t }}
  </label>
  <div class="{{ class_prefix }}__filters">
    {%- for filter in filters -%}
      {%
        render 'faceted-filter-group',
        filter: filter,
        class_prefix: class_prefix.
        forloop: forloop
      %}
    {%- endfor -%}
  </div>
</div>

{% if settings.favicon != blank %}
  <link rel="shortcut icon" href="{{ settings.favicon | img_url: '32x32' }}" type="image/png">
{% endif %}
{% liquid
  assign footerModules = 0

  if heading != blank or text != blank
    assign footerModules = footerModules | plus: 1
  endif

  unless settings.x-url.size == 0 and settings.facebook-url.size == 0 and settings.pinterest-url.size == 0 and settings.instagram-url.size == 0
    assign footerModules = footerModules | plus: 1
  endunless

  if mailingList
    assign footerModules = footerModules | plus: 1
  endif

  assign show_follow_on_shop = false
  assign country_selector = section.settings.show_currency_selector
  assign locale_selector = section.settings.show_locale_selector
  assign show_payment_icons = false
  assign right_column_item_count = 0
  assign additional_sub_footer_right_class = ''

  if shop.features.follow_on_shop? and section.settings.show_follow_on_shop
    assign show_follow_on_shop = true
  endif

  if shop.enabled_payment_types.size > 0 and section.settings.footer-payment-options
    assign show_payment_icons = true
  endif

  if country_selector or locale_selector
    assign right_column_item_count = right_column_item_count | plus: 1
  endif

  if show_payment_icons
    assign right_column_item_count = right_column_item_count | plus: 1
  endif

  if right_column_item_count == 1
    if country_selector or locale_selector
      assign additional_sub_footer_right_class = 'sub-footer-right--selectors-only'
    else
      assign additional_sub_footer_right_class = 'sub-footer-right--payments-only'
    endif
  endif
%}

