{% assign brandNames = settings.show-brand-names %}

<table class="cart-table table" data-cart-table>
  <thead>
    <tr class="table-header">
      <th class="first" colspan=2">{{ 'general.product' | t }}</th>
      <th class="cart-item-column-price">{{ 'general.price' | t }}</th>
      <th class="cart-item-column-quantity">{{ 'general.quantity' | t }}</th>
      <th class="cart-item-column-total last">{{ 'general.total' | t }}</th>
    </tr>
  </thead>

  <tbody>
    {% for item in cart.items %}
      {%- assign item_has_discounts = false -%}
      {%- if item.line_level_discount_allocations.size > 0 -%}
        {%- assign item_has_discounts = true -%}
      {%- endif -%}
      <tr
        class="
          table-row
          cart-item
          {% render 'for-looper', forloop: forloop %}
        "
        data-cart-item
        data-cart-item-key="{{ item.key }}"
      >
        <td
          class="
            cart-item-column
            product-image
            first
          "
        >
          <div class="image-wrap">
            <a
              class="image"
              href="{{ item.url }}"
              aria-label="{{ item.product.title }}"
            >
              {%
                render 'rimg'
                img: item.image,
                size: '160x160',
                lazy: true,
              %}

              {% if media_borders %}
                <span class="outline"></span>
              {% endif %}
            </a>
          </div>
        </td>
        <td
          class="
            cart-item-column
            product-item
          "
        >

          <div class="wrap">
            {% if brandNames %}
              <span class="label vendor">{{ item.vendor }}</span>
            {% endif %}

            <span class="label title">{{ item.product.title | link_to: item.url }}</span>

            {%- if item.product.has_only_default_variant == false or item.properties.size != 0 -%}
              <dl class="cart-item__product-options">
                {%- if item.product.has_only_default_variant == false -%}
                  {%- for option in item.options_with_values -%}
                    <div class="product-option">
                      <dt class="product-option__name">
                        {{- option.name -}}:
                      </dt>
                      <dd class="product-option__value">
                        {{- option.value -}}
                      </dd>
                    </div>
                  {%- endfor -%}
                {%- endif -%}

                {%- for property in item.properties -%}
                  {%- assign property_first_char = property.first | slice: 0 -%}
                  {%- if property.last != blank and property_first_char != '_' -%}
                    <div class="product-option">
                      <dt class="product-option__name">
                        {{- property.first -}}:
                      </dt>
                      <dd class="product-option__value">
                        {%- if property.last contains '/uploads/' -%}
                          <a href="{{ property.last }}" target="_blank">
                            {{- property.last | split: '/' | last -}}
                          </a>
                        {%- else -%}
                          {{- property.last -}}
                        {%- endif -%}
                      </dd>
                    </div>
                  {%- endif -%}
                {%- endfor -%}
              </dl>
            {%- endif -%}

            {% if item.selling_plan_allocation.selling_plan %}
              <span class="label cart-item__selling-plan">{{ item.selling_plan_allocation.selling_plan.name }}</span>
            {% endif %}

            {%- if item_has_discounts -%}
              <ul class="cart-item-discounts">
                {% for discount_allocation in item.line_level_discount_allocations %}
                  <li>
                    {% render 'icon' with 'sale-tag' %}
                    {{ discount_allocation.discount_application.title }}
                    (-<span class="money">{{ discount_allocation.amount | money }}</span>)
                  </li>
                {% endfor %}
              </ul>
            {%- endif -%}
          </div>
        </td>

        <td
          class="
            cart-item-column
            cart-item-column-price
            price
          "
        >
          <span class="cart-item-label-mobile">
            {{ 'general.price' | t }}
          </span>

          <div class="cart-item-column-contents">
            {%- if item_has_discounts -%}
              <s class="cart-item-original-price money">
                {{ item.original_price | money }}
              </s>
            {%- endif -%}
            <span class="cart-item-final-price money">
              {{ item.final_price | money }}
            </span>
            {% if item.unit_price_measurement %}
              {% capture total_quantity %}<span>{{ item.unit_price_measurement.quantity_value }}{{ item.unit_price_measurement.quantity_unit }}</span>{% endcapture %}
              {% capture unit_price %}<span>{{ item.unit_price | money }}</span>{% endcapture %}
              {% capture unit_measure %}<span>{%- if item.unit_price_measurement.reference_value != 1 -%}{{ item.unit_price_measurement.reference_value }}{%- endif %}{{ item.unit_price_measurement.reference_unit }}</span>{% endcapture %}

              <div class="cart-item__unit-price">
                {{ 'products.product.price_per_unit_html' | t: total_quantity: total_quantity, unit_price: unit_price, unit_measure: unit_measure | strip_newlines }}
              </div>
            {% endif %}
          </div>
        </td>

        <td
          class="
            cart-item-column
            cart-item-column-quantity
            quantity
          "
        >
          <span class="cart-item-label-mobile">
            {{ 'general.quantity' | t }}
          </span>

          <div class="cart-item-column-contents">
            <input
              class="
                cart-item-quantity
                field
              "
              type="number"
              name="updates[]"
              pattern="\d*"
              value="{{ item.quantity }}"
              data-cart-item-quantity
              aria-label="{{ 'general.quantity' | t }}"
            >
            <a
              class="cart-item-remove"
              href="{{ routes.cart_change_url }}/{{ item.variant.id }}?line={{ forloop.index }}&quantity=0"
              data-cart-action="remove"
            >
              {{ 'cart.remove' | t }}
            </a>
          </div>
        </td>

        <td
          class="
            cart-item-column
            cart-item-column-total
            total
            last
          "
        >
          <span class="cart-item-label-mobile">
            {{ 'general.total' | t }}
          </span>

          <div class="cart-item-column-contents">
            {%- if item_has_discounts -%}
              <s class="cart-item-original-price money">
                {{ item.original_line_price | money }}
              </s>
            {%- endif -%}
            <span class="cart-item-final-price money">
              {{ item.final_line_price | money }}
            </span>
          </div>
        </td>
      </tr>
    {% endfor %}
  </tbody>
</table>

{% comment %}
  @param onboarding {Boolean}
  @param collection
  @param link
{% endcomment %}

{% liquid
  assign collectionImage = 'collection-4' | placeholder_svg_tag: 'placeholder-svg'
  assign collectionTitle = 'onboarding.collection_title' | t
  assign link = link | default: false
  assign collection = collection | default: nil
  assign onboarding = onboarding | default: false

  unless onboarding
    if link
      assign currentCollection = link.object
    else
      assign currentCollection = collection
    endif

    if currentCollection == blank
      continue
    endif

    if currentCollection.image
      assign collectionImage = currentCollection.image.src
    else
      assign collectionImage = currentCollection.products.first.featured_media.preview_image
    endif

    assign collectionTitle = currentCollection.title
    assign collectionDescription = currentCollection.description
  endunless
%}

<div class="collection-wrap first">
  <a href="{{ currentCollection.url }}">
    {% unless onboarding %}
      <div class="collection"
        {%
          render 'rimg'
          img: collectionImage,
          size: '480x480',
          crop: 'center',
          background: true,
          lazy: true,
        %}
      >
        {%
          render 'rimg'
          img: collectionImage,
          size: '480x480',
          crop: 'center',
          lazy: true,
        %}
      </div>
    {% else %}
      <div class="collection collection-onboarding">
        {{- collectionImage -}}
      </div>
    {% endunless %}

    <h2 class="label preload">
      {{- collectionTitle -}}
    </h2>
  </a>

  {% if section.settings.show_collection_descriptions and collectionDescription != blank %}
    <div class="rte">
      {{- collectionDescription -}}
    </div>
  {% endif %}
</div>

