{% liquid
  assign age_gate_enabled = settings.enable_age_gate_site_wide
  assign page_min_age = settings.age_gate_site_wide_min_age

  if sections contains 'data-required-age'
    assign age_gate_enabled = true
    assign section_min_age = sections | split: 'data-required-age="||' | last | split: '||"' | first | times: 1

    if page_min_age < section_min_age
      assign page_min_age = section_min_age
    endif
  endif
%}

{% if age_gate_enabled %}
  <div id="age-gate-{{ id }}" class="age-gate"{% if request.design_mode %} style="display: none;"{% endif %}>
    <div class="age-gate__content">
      <div class="age-gate__header">
        {% if settings.show_age_gate_logo and shop.brand.logo != blank %}
          <div class="age-gate__logo-wrapper">
            {%
              render 'rimg',
              img: shop.brand.logo,
              alt: shop.name,
              size: '300x',
              lazy: true,
              canvas: true,
              class: 'age-gate__logo'
            %}
          </div>
        {% endif %}

        {% if settings.age_gate_heading != blank %}
          <p class="age-gate__heading">{{ settings.age_gate_heading }}</p>
        {% endif %}

        {% if settings.age_gate_description != blank %}
          <p class="age-gate__description">{{ settings.age_gate_description }}</p>
        {% endif %}
      </div>
      <form id="age-gate-{{ id }}-form" class="age-gate__form" data-age-gate data-required-age="{{ page_min_age }}">
        <div class="age-gate__select-wrapper">
          <label class="age-gate__select-label" for="age-gate-day">{{ 'age_gate.form.day' | t }}</label>
          <select id="age-gate-{{ id }}-day" class="age-gate__select age-gate__day" name="day" required>
            <option value="" selected>{{ 'age_gate.form.day' | t }}</option>
            {% for i in (1..31) %}
              <option value="{{ i }}">{{ i }}</option>
            {% endfor %}
          </select>
        </div>

        <div class="age-gate__select-wrapper">
          <label class="age-gate__select-label" for="age-gate-month">{{ 'age_gate.form.month' | t }}</label>
          <select id="age-gate-{{ id }}-month" class="age-gate__select age-gate__month" name="month" required>
            <option value="" selected>{{ 'age_gate.form.month' | t }}</option>
            <option value="0">{{ 'age_gate.form.january' | t }}</option>
            <option value="1">{{ 'age_gate.form.february' | t }}</option>
            <option value="2">{{ 'age_gate.form.march' | t }}</option>
            <option value="3">{{ 'age_gate.form.april' | t }}</option>
            <option value="4">{{ 'age_gate.form.may' | t }}</option>
            <option value="5">{{ 'age_gate.form.june' | t }}</option>
            <option value="6">{{ 'age_gate.form.july' | t }}</option>
            <option value="7">{{ 'age_gate.form.august' | t }}</option>
            <option value="8">{{ 'age_gate.form.september' | t }}</option>
            <option value="9">{{ 'age_gate.form.october' | t }}</option>
            <option value="10">{{ 'age_gate.form.november' | t }}</option>
            <option value="11">{{ 'age_gate.form.december' | t }}</option>
          </select>
        </div>

        <div class="age-gate__select-wrapper">
          <label class="age-gate__select-label" for="age-gate-year">{{ 'age_gate.form.year' | t }}</label>
          <select id="age-gate-{{ id }}-year" class="age-gate__select age-gate__year" name="year" required>
            <option value="" selected>{{ 'age_gate.form.year' | t }}</option>
            {% assign year = "now" | date: "%Y" | times: 1 %}
            {% assign startingYear = year | minus: 120 %}
            {% for i in (startingYear..year) %}
              {% assign j = year | minus: forloop.index0 %}
              <option value="{{ j }}">{{ j }}</option>
            {% endfor %}
          </select>
        </div>

        <button class="age-gate__confirm_btn" type="submit">{{ 'age_gate.form.confirm' | t: shop: shop.name }}</button>
        <p class="age-gate__error" data-age-gate-error style="display: none;">{{ 'age_gate.form.error' | t: age: page_min_age }}</p>
      </form>
    </div>
  </div>

  {% unless request.design_mode %}
    <script>
      const ageGateAge = sessionStorage.getItem('age-gate');

      if (ageGateAge >= {{ page_min_age }}) {
        const ageGateEl = document.getElementById('age-gate-{{ id }}');

        if (ageGateEl) {
          ageGateEl.style.display = 'none';
        }
      }
    </script>
  {% endunless %}
{% endif %}
{% assign article_item = article_item | default: false %}

{% capture article_author %}
    <div class="article-author">
      {% if article.user.image != empty and article.user.email != blank %}
        <div class="article-author-avatar">
          {% capture avatar %}
            {% if article.user.image %}
              {%
                render 'rimg',
                img: article.user.image,
                class: 'article-author-image',
                lazy_load: true,
                size: '54x',
                
              %}
            {% else %}
              <img
                class="article-author-image"
                src="//www.gravatar.com/avatar/{{ article.user.email | remove: ' ' | strip_newlines | downcase | md5 }}"
                alt="{{ 'blogs.article_item.post_author' | t }}"
                width='400px'
                height='400px'
                lazy_load: true
              />
            {% endif %}
          {% endcapture %}

          {% if article.user.homepage !=  blank %}
            <a href="{{ article.user.homepage }}">
              {{ avatar }}
            </a>
          {% else %}
            {{ avatar }}
          {% endif %}
        </div>
      {% endif %}

      <span class="article-author-writtenby">
        {{- 'blogs.article_item.written_by' | t -}}
      </span>

      <span class="article-author-name">
        {{- article.user.name -}}
      </span>
    </div>
{% endcapture %}

<article class="article">
  <div class="article-meta">
    {% if show_author %}
      {{ article_author }}
    {% else %}
      <span class="article-author__placeholder"></span>
    {% endif %}

    {% if article.tags.size > 0 and show_tags %}
      <div class="article-tags">
        <span class="article-tags-filedunder">{{ 'blogs.article_item.filed_under' | t }}</span>

        <div class="article-tags-tags">
          {% for tag in article.tags %}
            <a href="{{ blog.url }}/tagged/{{ tag | handle }}">{{ tag }}</a>{% unless forloop.last %},{% endunless %}
          {% endfor %}
        </div>
      </div>
    {% endif %}

    {% if article_item and article.comments_enabled? and article.comments.size > 0 %}
      <div class="article-comments">
        <span class="article-comments-discussion">{{ 'blogs.article_item.discussion' | t }}</span>

        <span class="article-comments-count">
          <a
            href="{{ article.url }}#comments"
          >
            {{ article.comments_count }} {{ 'blogs.article_item.comment_count' | t: count: article.comments_count }}
          </a>
        </span>
      </div>
    {% endif %}

    {% if blog.next_article or blog.previous_article %}
      {% assign current_found = false %}
      {% assign done = false %}
      {% for a in blog.articles %}
        {% if current_found and done == false %}
          {% assign next_article = a %}
          {% assign done = true %}
        {% endif %}

        {% unless done %}
          {% if a.id == article.id %}
            {% assign current_found = true %}
          {% else %}
            {% assign prev_article = a %}
          {% endif %}
        {% endunless %}
      {% endfor %}

      <div class="article-pagination">
        {% if prev_article %}
          <a class="article-pagination--prev" href="{{ prev_article.url }}">
            {{ 'general.pagination.previous' | t }}
          </a>
        {% endif %}

        {% if prev_article and next_article %}
          <span class="sep">/</span>
        {% endif %}

        {% if next_article %}
          <a class="article-pagination--next" href="{{ next_article.url }}">
            {{ 'general.pagination.next' | t }}
          </a>
        {% endif %}
      </div>
    {% endif %}

    {% unless article_item %}
      {% if section.settings.show-share %}
        <div class="article-share">
          {% assign article_canonical_url = shop.url | append: article.url %}
          {% assign article_email_subject = 'articles.article.share_email_subject' | t: article: article.title %}
          {% assign article_email_body = 'articles.article.share_email_body' | t: article: article.title, article_url: article_canonical_url %}
          {% assign article_image = article.image | default: page_image | img_url: '1024x1024' %}
          {% assign article_description = article.title | escape %}

          {%
            render 'share-this',
            share_canonical_url: article_canonical_url,
            email_subject: article_email_subject,
            email_body: article_email_body,
            image: article_image,
            description: article_description,
          %}
        </div>
      {% endif %}
    {% endunless %}
  </div>

  <div class="article-content">
    <header class="article-header">
      <h1 class="article-title">
        {% if article_item %}
          <a href="{{ article.url }}">{{ article.title }}</a>
        {% else %}
          {{ article.title }}
        {% endif %}
      </h1>

      {% if show_date %}
        <div class="article-date">
          <label>
            {{- article.published_at | date: format: 'short_month' -}}
          </label>
        </div>
      {% endif %}

      {% if show_author %}
        {{ article_author }}
      {% endif %}

      {% if blog.next_article or blog.previous_article %}
        {% if prev_article %}
          <a class="article-pagination--prev" href="{{ prev_article.url }}">
            {% render 'icon' with 'arrow-thin-left' %}
          </a>
        {% endif %}

        {% if next_article %}
          <a class="article-pagination--next" href="{{ next_article.url }}">
            {% render 'icon' with 'arrow-thin-right' %}
          </a>
        {% endif %}
      {% endif %}
    </header>

    {% if article.image and show_featured_image %}
      <div class="article-image">
        {% capture article_image %}
          {%
            render 'rimg'
            img: article.image,
            alt: article.title,
            size: '1024x1024',
            lazy: true,
          %}
        {% endcapture %}

        {% if article_item %}
          <a href="{{ article.url }}">
            {{ article_image }}
          </a>
        {% else %}
          {{ article_image }}
        {% endif %}
      </div>
    {% endif %}

    <div class="article-rte rte clearfix">
      {% if article_item %}
        {% if show_excerpt %}
          {{- article.excerpt_or_content -}}
        {% endif %}

        <a
          class="article-rte-continuereading"
          href="{{ article.url }}"
        >
          {{ 'blogs.article_item.continue_reading' | t }}
        </a>
      {% else %}
        {{ article.content }}
      {% endif %}
    </div>

    {% unless article_item %}
      {% if article.comments_enabled? %}
        {% if article.comments.size > 0 %}
          <div class="comments-wrap read {% if article.comments_count == 0 %}no-comments{% endif %}" id="comments">
            <h2 class="title">
              {% capture comment_count %}
                {% if comment and comment.created_at %}
                  <span class="count">({{ article.comments_count | plus: 1 }})</span>
                {% else %}
                  <span class="count">({{ article.comments_count }})</span>
                {% endif %}
              {% endcapture %}
              {{ 'blogs.article_item.comments_html' | t: comment_count: comment_count }}
            </h2>

            {% if article.comments_count > 0 %}
            <ul class="comments">
              {% paginate article.comments by 10 %}
                {% for comment in article.comments %}
                <li class="comment {% render 'for-looper', forloop: forloop %}" id="comment-{{ comment.id }}">
                  <a href="{{ comment.url }}" class="date">{{ comment.created_at | date: format: 'short_month' }}</a>
                  <div class="body">{{ comment.content }}</div>
                  <p class="author">â€” {{ comment.author }}</p>
                </li>
                {% endfor %}

                {% if paginate.previous or paginate.next %}
                  {%
                    render 'pagination',
                    paginate: paginate,
                    paginationStyle: section.settings.pagination-style
                  %}
                {% endif %}
              {% endpaginate %}
            </ul>
            {% endif %}
          </div>
        {% endif %}

        <div
          class="comments-wrap submit"
          {% if article.comments.size == 0 %}
            id="comments"
          {% endif %}
        >
          <h2 class="title">{{ 'blogs.article_item.leave_comment' | t }}</h2>

          {% form article %}
            {% if form.errors %}
              <div class="error-message">
                <span>{{ 'blogs.article_item.error_message' | t }}</span>
              </div>
            {% endif %}

            {% if form.posted_successfully? %}
              <div class="alert-message success">
                <span>{{ 'blogs.article_item.success_message' | t }}</span>
              </div>
            {% endif %}

            <div class="field-wrap author">
              <label for="comment-form__author">
                {{- 'general.name' | t -}}
              </label>

              <input
                class="
                  field
                  {% if form.errors contains 'author' %}
                    error
                  {% endif %}
                "
                id="comment-form__author"
                type="text"
                name="comment[author]"
                placeholder="{{ 'general.name_placeholder' | t }}"
                value=""
                required
              >
            </div>

            <div class="field-wrap email">
              <label for="comment-form__email">
                {{- 'general.email' | t -}}
              </label>

              <input
                class="
                  field
                  {% if form.errors contains 'email' %}
                    error
                  {% endif %}
                "
                id="comment-form__email"
                type="text"
                name="comment[email]"
                placeholder="{{ 'general.email_placeholder' | t }}"
                value=""
                required
              >
            </div>

            <div class="field-wrap body">
              <label for="comment-form__message">
                {{- 'general.message' | t -}}
              </label>

              <textarea
                class="
                  field
                  {% if form.errors contains 'body' %}
                    error
                  {% endif %}
                "
                id="comment-form__message"
                name="comment[body]"
                required
              ></textarea>
            </div>

            <input
              class="
                action-button
                submit-button
              "
              type="submit"
              value="{{ 'general.submit' | t }}"
            >
          {% endform %}
        </div>
      {% endif %}
    {% endunless %}
  </div>
</article>

<div
  class="
    back-to-top
    back-to-top--desktop
    back-to-top-position--{{ settings.back_to_top_desktop_position }}
  "
  data-back-to-top
>
  <a
    class="back-to-top__button"
    href="#"
    role="button"
    aria-label="{{ 'general.back_to_top' | t }}"
    aria-hidden="true"
    data-back-to-top-button
  >
    {% render 'icon' with 'down-arrow' %}
  </a>
</div>

<div class="back-to-top back-to-top--mobile">
  <a
    class="back-to-top__button"
    href="#"
    role="button"
  >
    <span>{{ 'general.back_to_top' | t }}</span>
    <span class="back-to-top__icon-wrapper">
      {% render 'icon' with 'arrow-thin-left' %}
    </span>
  </a>
</div>


{%- if item -%}
    {%- assign bb_price = item.price -%}
    {%- assign bb_line_price = item.line_price -%}
    {%- assign bb_final_line_price = item.final_line_price -%}
    {%- assign bb_final_price = item.final_price -%}
    {%- assign bb_original_line_price = item.original_line_price -%}
    {%- assign bb_original_price = item.original_price -%}

    {% assign price_key = '_bundle_price' %}
    {%- capture price_with_currency_key -%}{{ price_key }}-{{ cart.currency.iso_code }}{%- endcapture -%}
    {% assign original_price_key = '_original_bundle_price' %}
    {%- capture original_price_with_currency_key -%}{{ original_price_key }}-{{ cart.currency.iso_code }}{%- endcapture -%}

    {%- for property in item.properties -%}
        {%- if property.first == price_key -%}
            {%- assign bb_price = property.last -%}
            {%- assign bb_final_price = bb_price -%}
            {%- assign bb_line_price = bb_price | times: item.quantity -%}
            {%- assign bb_final_line_price = bb_line_price -%}
            {%- assign bb_original_price = bb_price -%}
            {%- assign bb_original_line_price = bb_line_price -%}
        {%- endif -%}
    {%- endfor -%}

    {%- for property in item.properties -%}
        {%- if property.first == price_with_currency_key -%}
            {%- assign bb_price = property.last -%}
            {%- assign bb_final_price = bb_price -%}
            {%- assign bb_line_price = bb_price | times: item.quantity -%}
            {%- assign bb_final_line_price = bb_line_price -%}
            {%- assign bb_original_price = bb_price -%}
            {%- assign bb_original_line_price = bb_line_price -%}
        {%- endif -%}
    {%- endfor -%}

    {%- for property in item.properties -%}
        {%- if property.first == original_price_key -%}
            {%- assign bb_original_price = property.last -%}
            {%- assign bb_original_line_price = bb_original_price | times: item.quantity -%}
        {%- endif -%}
    {%- endfor -%}

    {%- for property in item.properties -%}
        {%- if property.first == original_price_with_currency_key -%}
            {%- assign bb_original_price = property.last -%}
            {%- assign bb_original_line_price = bb_original_price | times: item.quantity -%}
        {%- endif -%}
    {%- endfor -%}

    {%- if type == 'bb_price' -%}
        {{ bb_price }}
    {%- elsif type == 'bb_line_price' -%}
        {{ bb_line_price }}
    {%- elsif type == 'bb_final_line_price' -%}
        {{ bb_final_line_price }}
    {%- elsif type == 'bb_final_price' -%}
        {{ bb_final_price }}
    {%- elsif type == 'bb_original_line_price' -%}
        {{ bb_original_line_price }}
    {%- elsif type == 'bb_original_price' -%}
        {{ bb_original_price }}
    {%- endif -%}
{%- elsif type == 'cart' -%}
    {%- assign bb_prefix = "/apps/bundles" -%}
    <input type="hidden" name="_cart" value="{{ cart | json | escape }}" />
    <input type="hidden" name="_shop" value="{{ shop.permanent_domain | escape }}" />
    <input type="hidden" id="bb_proxy_prefix" value="{{ bb_prefix }}" />
{%- elsif type == 'bb_checkout_link' -%}
    {%- assign bb_contains_bundle = false -%}
    {%- assign bb_prefix = "/apps/bundles" -%}

    {%- for item in cart.items -%}
        {%- for property in item.properties -%}
            {%- if property.first == '_bundle_price' -%}
                {%- assign bb_contains_bundle = true -%}
                {%- break -%}
            {%- endif -%}
        {%- endfor -%}

        {%- if bb_contains_bundle -%}
            {%- break -%}
        {%- endif -%}
    {%- endfor -%}

    {%- if bb_contains_bundle -%}
        {{ bb_prefix }}/cart
    {%- endif -%}
{%- else -%}
    {%- assign bb_total_price = 0 -%}
    {%- assign bb_original_total_price = 0 -%}

    {% assign price_key = '_bundle_price' %}
    {%- capture price_with_currency_key -%}{{ price_key }}-{{ cart.currency.iso_code }}{%- endcapture -%}
    {% assign original_price_key = '_original_bundle_price' %}
    {%- capture original_price_with_currency_key -%}{{ original_price_key }}-{{ cart.currency.iso_code }}{%- endcapture -%}

    {%- for item in cart.items -%}
        {%- assign _final_price = item.final_line_price -%}
        {%- assign _original_price = item.original_line_price -%}
        {%- for property in item.properties -%}
            {%- if property.first == price_key -%}
                {%- assign _final_price = property.last | times: item.quantity -%}
                {%- assign _original_price = property.last | times: item.quantity -%}
            {%- endif -%}
        {%- endfor -%}
        {%- for property in item.properties -%}
            {%- if property.first == price_with_currency_key -%}
                {%- assign _final_price = property.last | times: item.quantity -%}
                {%- assign _original_price = property.last | times: item.quantity -%}
            {%- endif -%}
        {%- endfor -%}
        {%- for property in item.properties -%}
            {%- if property.first == original_price_key or property.first == original_price_with_currency_key -%}
                {%- assign _original_price = property.last | times: item.quantity -%}
            {%- endif -%}
        {%- endfor -%}
        {%- for property in item.properties -%}
            {%- if property.first == original_price_with_currency_key -%}
                {%- assign _original_price = property.last | times: item.quantity -%}
            {%- endif -%}
        {%- endfor -%}
        {%- assign bb_total_price = bb_total_price | plus: _final_price -%}
        {%- assign bb_original_total_price = bb_original_total_price | plus: _original_price -%}
    {%- endfor -%}

    {% assign bb_items_subtotal_price = bb_total_price %}
    {%- for discount_application in cart.cart_level_discount_applications -%}
        {%- assign bb_total_price = bb_total_price | minus: discount_application.total_allocated_amount -%}
    {% endfor %}

    {%- if type == 'bb_total_price' -%}
        {{ bb_total_price }}
    {%- elsif type == 'bb_original_total_price' -%}
        {{ bb_original_total_price }}
    {%- elsif type == 'bb_items_subtotal_price' -%}
        {{ bb_items_subtotal_price }}
    {%- endif -%}
{%- endif -%}

<div class="blog-sidebar">
  {% if display_recent_posts %}
    <div class="blog-sidebar-recentposts">
      <span class="blog-sidebar-recentposts-title">{{ 'blogs.blog_sidebar.recent_posts' | t }}</span>

      <div class="blog-sidebar-recentposts-posts">
        {% for article in blog.articles limit: 5 %}
          <div class="blog-sidebar-recentposts-post">
            <a class="blog-sidebar-recentposts-post-title" href="{{ article.url }}">{{ article.title }}</a>

            <span class="blog-sidebar-recentposts-post-date">{{ article.published_at | date: format: 'short_month' }}</span>
          </div>
        {% endfor %}
      </div>
    </div>
  {% endif %}

  {% if display_tags and blog.all_tags.size > 0 %}
    <div class="blog-sidebar-tags">
      <span class="blog-sidebar-tags-title">{{ 'blogs.blog_sidebar.tags' | t }}</span>

      <div class="blog-sidebar-tags-tags">
        {% for tag in blog.all_tags %}
          <div class="blog-sidebar-tags-tag">
            <a href="{{ blog.url }}/tagged/{{ tag | handle }}">
              {{ tag }}
            </a>
          </div>
        {% endfor %}
      </div>
    </div>
  {% endif %}
</div>

<div class="breadcrumb-navigation">
  <span><a href="{{ routes.root_url }}">Home</a></span>
  {% if template contains "product" %}
    {% if collection %}
      <span class="sep">/</span> <span><a href="{{ routes.collections_url }}">{{ 'general.breadcrumbs.collections' | t }}</a></span> <span class="sep">/</span> <span><a href="{{ collection.url }}">{{ collection.title }}</a></span>
    {% else %}
      <span class="sep">/</span> <span><a href="{{ routes.all_products_collection_url }}">{{ 'general.breadcrumbs.products' | t }}</a></span>
    {% endif %}
    <span class="sep">/</span> <span>{{ product.title }}</span>
  {% elsif template contains "list-collections" %}
    <span class="sep">/</span> <span>{{ 'collections.listing.header' | t }}</span>
  {% elsif template contains "collection" %}
    <span class="sep">/</span> <span><a href="{{ routes.collections_url }}">{{ 'general.breadcrumbs.collections' | t }}</a></span> <span class="sep">/</span> <span>{{ collection.title }}</span>
  {% elsif template contains "blog" %}
    <span class="sep">/</span> <span>{{ blog.title | link_to: blog.url }}</span>
    {% if current_tags.size > 0 %}
    <span class="sep">/</span> <span>{{ 'general.breadcrumbs.tagged' | t }}: {% for tag in current_tags %}{{ tag }}{% unless forloop.last %}, {% endunless %}{% endfor %}</span>
    {% endif %}
  {% elsif template contains "page" %}
    <span class="sep">/</span> <span>{{ page.title }}</span>
  {% elsif template contains "article" %}
    <span class="sep">/</span> <span>{{ blog.title | link_to: blog.url }}</span>
    <span class="sep">/</span> <span>{{ article.title }}</span>
  {% elsif template contains "cart" %}
    <span class="sep">/</span> <span>{{ 'general.breadcrumbs.cart' | t }}</span>
  {% elsif template contains "404" %}
